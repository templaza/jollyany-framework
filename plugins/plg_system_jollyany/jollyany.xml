<?xml version="1.0" encoding="utf-8"?>
<extension version="3.1" type="plugin" method="upgrade" group="system">
   <name>Jollyany Plugin</name>
   <author>TemPlaza</author>
   <creationDate>December 2023</creationDate>
   <version>3.5.21</version>
   <url>https://www.jollyany.co</url>
   <copyright>Copyright (C) 2021 TemPlaza, Jsc. All rights reserved.</copyright>
   <license>GNU General Public License version 2 or later; see LICENSE.txt</license>
   <authorEmail>info@templaza.com</authorEmail>
   <authorUrl>https://www.templaza.com</authorUrl>
   <description>Jollyany Framework Plugin Description Here</description>>
   <files>
      <filename plugin="jollyany">jollyany.php</filename>
   </files>
   <config>
      <fields name="params">
         <fieldset name="basic">
            <field name="jollyany_license" type="hidden" default="" />
            <field name="jollyany_load_uikit" type="radio" default="cdn" label="JOLLYANY_LOAD_UIKIT" description="JOLLYANY_LOAD_UIKIT_DESC" class="btn-group btn-group-yesno">
               <option value="cdn">CDN</option>
               <option value="local">Local</option>
            </field>
         </fieldset>
      </fields>
   </config>
   <updateservers>
      <server type="extension" name="Jollyany Plugin" priority="1">https://www.templaza.com/tzupdates/jollyany.xml</server>
   </updateservers>
</extension>